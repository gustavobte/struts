<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.keylesson.persistence.Estabelecimento" table="estabelecimento">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="identity" />
		</id>
		<property name="estabelecimento" type="string">
			<column name="ESTABELECIMENTO" length="20" not-null="true" />
		</property>
		<property name="situacao" type="string">
			<column name="SITUACAO" />
		</property>
	</class>
</hibernate-mapping>